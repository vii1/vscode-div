{
    "name": "div",
    "displayName": "DIV",
    "description": "Language support for DIV Games Studio",
    "version": "0.1.2",
    "icon": "icon.png",
    "author": {
        "name": "vii",
        "email": "vii@divero.net"
    },
    "repository": {
        "url": "https://github.com/vii1/vscode-div.git"
    },
    "publisher": "vscode-div",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "--main": "./out/extension.js",
    "--activationEvents": [
        "onLanguage:div"
    ],
    "contributes": {
        "languages": [
            {
                "id": "div",
                "aliases": [
                    "DIV",
                    "div"
                ],
                "extensions": [
                    ".prg"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "div",
                "scopeName": "source.div",
                "path": "./syntaxes/div.tmLanguage.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^10.14.17",
        "@types/vscode": "^1.34.0",
        "tslint": "^5.16.0",
        "typescript": "^3.5.1"
    },
    "dependencies": {
        "parser": "^0.1.4",
        "tokenizer": "git+https://github.com/Floby/node-tokenizer.git"
    }
}
