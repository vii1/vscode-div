{
    "name": "div",
    "displayName": "DIV",
    "description": "Language support for DIV Games Studio",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "div",
                "aliases": [
                    "DIV",
                    "div"
                ],
                "extensions": [
                    ".prg"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "div",
                "scopeName": "source.div",
                "path": "./syntaxes/div.tmLanguage.json"
            }
        ]
    }
}
